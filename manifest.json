{
    "name": "NepAd Blocker",
    "version": "1.0",
    "description": "Block Ads of Nepal Sites",
    "content_scripts": [
        {
          "matches": ["*://*.onlinekhabar.com/*"],
          "run_at": "document_idle",
          "js": ["content.js"]
          
        }
      ],

    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
    "background": {
        "scripts": ["app.js"]
    },

    "icons": {
        "16": "icons/logo_16x16.png",
        "48": "icons/logo_48x48.png",
        "96": "icons/logo_96x96.png"
    },
    "manifest_version": 2
}